				<Source><![CDATA[
    private static void deleteRecordFromFeedbackTable(str _environmentName, str _flowName, str _flowRunId)
    {
        DAXMobileWorkflowFeedbackTable::deleteRecord(
        _environmentName,
        _flowName,
        _flowRunId);
    }

]]></Source>
